@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix gr: <http://purl.org/goodrelations/v1#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <https://w3id.org/ibp/GoodRelationsPropertyPatternOntology> .

<https://w3id.org/ibp/GoodRelationsPropertyPatternOntology> rdf:type owl:Ontology ;
    dcterms:creator "Georg Ferdinand Schneider" ;
    dcterms:creator <https://www.researchgate.net/profile/Georg_Schneider3> ;
	dcterms:license <http://creativecommons.org/licenses/by-sa/4.0> .

gr:QualitativeValue rdf:type owl:Class .
gr:QuantitativeValue rdf:type owl:Class .
gr:ProductOrService rdf:type owl:Class .

# Datatype property
gr:dataTypeProductOrServiceProperty rdf:type owl:DatatypeProperty .
gr:ProductOrService gr:dataTypeProductOrServiceProperty rdfs:Literal .

# Object properties
gr:qualitativeProductOrServiceProperty rdf:type owl:ObjectProperty .
gr:ProductOrService gr:qualitativeProductOrServiceProperty gr:QualitativeValue .

gr:quantitativeProductOrServiceProperty rdf:type owl:ObjectProperty .
gr:ProductOrService gr:quantitativeProductOrServiceProperty gr:QuantitativeValue .

gr:weight rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf gr:quantitativeProductOrServiceProperty .          

gr:valueReference rdf:type owl:ObjectProperty .
gr:QuantitativeValue gr:valueReference gr:QualitativeValue .
gr:QualitativeValue gr:valueReference gr:QuantitativeValue .
